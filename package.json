{
	"name": "uw-faculty-page",
	"version": "1.0.5",
	"description": "The webpage for University of Washington Professor Amy J. Ko.",
	"license": "MIT License",
	"repository": "https://github.com/amyjko/faculty.git",
	"scripts": {
		"lint": "ajv -s profile/schema.json -c ajv-formats -d profile/profile.json",
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"dry": "rsync --dry-run -avz --delete --exclude-from='deploy-excludes' . ajko@ovid.u.washington.edu:~/public_html",
		"ship": "npm run build; rsync -avz --delete --exclude-from='deploy-excludes' . ajko@ovid.u.washington.edu:~/public_html"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@types/smoothscroll-polyfill": "^0.3.1",
		"ajv-cli": "^5.0",
		"ajv-formats": "^2.0.2",
		"svelte": "^3.44.0",
		"svelte-check": "^2.7.1",
		"svelte-preprocess": "^4.10.6",
		"tslib": "^2.3.1",
		"typescript": "^4.7.4",
		"vite": "^3.1.0"
	},
	"type": "module"
}